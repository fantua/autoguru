{
  "name": "autoguru",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run _server & npm run _build-dev",
    "debug": "npm run _server & npm run _build-debug ",
    "_server": "live-server --host=192.168.0.102 --port=3000 --ignore=src --no-browser --entry-file=index.html",
    "build": "NODE_ENV=production browserify src/main.jsx --extension=jsx | uglifyjs --screw-ie8 -c=dead_code,evaluate,loops,unused > build/bundle.js",
    "_build-dev": "NODE_ENV=development watchify src/main.jsx --extension=jsx -o build/bundle.js -p [ livereactload --host localhost ] -dv",
    "_build-debug": "NODE_ENV=development watchify src/main.jsx --extension=jsx -o build/bundle.js -dv"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ],
      "envify"
    ]
  },
  "dependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^12.0.2",
    "classnames": "^2.2.3",
    "envify": "^3.4.0",
    "jquery": "^2.2.2",
    "parse": "^1.8.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-router": "^2.0.1",
    "uglify-js": "^2.6.2"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.2",
    "live-server": "^0.9.2",
    "livereactload": "^2.2.2",
    "node-static": "^0.7.7",
    "react-proxy": "^1.1.8",
    "watchify": "^3.7.0"
  }
}
